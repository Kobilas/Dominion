import javax.swing.*;
import java.awt.*;
public class Dominion extends JFrame
{
    public JPanel gp = (JPanel) getGlassPane();
    public Images i;
    public Keying k;
    public PowerUp p;
    public Dominion()
    {
        i = new Images();
        gp.setVisible(true);
        k = new Keying(this, i);
        gp.setLayout(new GridLayout(1, 1, 0, 0));
        this.setLayout(new GridLayout(1, 1, 0, 0));
        gp.add(k);
        this.add(i);
        p = new PowerUp();
        this.add(p);
        gp.add(p);
    }
}
